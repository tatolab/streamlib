{
  "name": "microphone-reverb-speaker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/microphone-reverb-speaker/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo build",
        "cwd": "examples/microphone-reverb-speaker"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run",
        "cwd": "examples/microphone-reverb-speaker"
      }
    }
  },
  "tags": ["rust", "example", "audio", "clap"]
}
