{
  "name": "simple-pipeline",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/simple-pipeline/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo build",
        "cwd": "examples/simple-pipeline"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run",
        "cwd": "examples/simple-pipeline"
      }
    }
  },
  "tags": ["rust", "example"]
}
