{
  "name": "camera-audio-recorder",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/camera-audio-recorder/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo build",
        "cwd": "examples/camera-audio-recorder"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run",
        "cwd": "examples/camera-audio-recorder"
      }
    }
  },
  "tags": ["rust", "example", "camera", "audio", "mp4", "recording"]
}
